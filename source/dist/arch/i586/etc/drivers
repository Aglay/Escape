vga
	/dev/vga 0770 output
vesa
	/dev/vesa 0770 output
zero
	/dev/zero 0666 root
random
	/dev/random 0666 root
null
	/dev/null 0666 root
speaker
	/dev/speaker 0110 audio
keyboard
	/dev/keyboard 0110 input
mouse
	/dev/mouse 0110 input
uimanager /dev/vga /dev/vesa
	/dev/uim-ctrl 0110 input
	/dev/uim-input 0110 input
