output /sbin/video
	/dev/vga 0110 output
	/dev/vesa 0110 output
util /sbin/zero
	/dev/zero 0660 util
util /sbin/random
	/dev/random 0660 util
util /sbin/null
	/dev/null 0660 util
util /sbin/pipe
	/dev/pipe 0660 util
audio /sbin/speaker
	/dev/speaker 0110 audio
input /sbin/ps2
	/dev/keyb 0110 input
	/dev/mouse 0110 input
root /sbin/tcpip
	/dev/socket 0770 network
	/dev/tcpip 0110 network
network /sbin/http
	/dev/http 0440 network
ui /sbin/uimng /dev/vga /dev/vesa
	/dev/uimng 0110 input
