/sbin/video
	/dev/vga 0770 output
/sbin/zero
	/dev/zero 0666 root
/sbin/random
	/dev/random 0666 root
/sbin/null
	/dev/null 0666 root
/sbin/pipe
	/dev/pipe 0666 root
/sbin/keyb
	/dev/keyb 0110 input
/sbin/tcpip
	/dev/socket 0770 network
	/dev/tcpip 0110 network
/sbin/term 0
	/dev/term0 0770 output
/bin/login TERM=/dev/term0
/sbin/uimng /dev/vga
	/dev/uimng 0110 input
