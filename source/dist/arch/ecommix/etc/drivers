output /sbin/video
	/dev/vga 0770 output
util /sbin/zero
	/dev/zero 0666 root
util /sbin/random
	/dev/random 0666 root
util /sbin/null
	/dev/null 0666 root
util /sbin/pipe
	/dev/pipe 0666 root
input /sbin/keyb
	/dev/keyb 0110 input
root /sbin/tcpip
	/dev/socket 0770 network
	/dev/tcpip 0110 network
ui /sbin/term 0
	/dev/term0 0770 output
root /bin/login TERM=/dev/term0
root /sbin/uimng /dev/vga
	/dev/uimng 0110 input
