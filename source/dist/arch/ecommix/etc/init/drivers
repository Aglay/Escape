output /sbin/video
	/dev/vga 0770 output
util /sbin/zero
	/dev/zero 0666 util
util /sbin/random
	/dev/random 0666 util
util /sbin/null
	/dev/null 0666 util
util /sbin/pipe
	/dev/pipe 0666 util
input /sbin/keyb
	/dev/keyb 0110 input
netstack /sbin/tcpip
	/dev/socket 0770 netstack
	/dev/tcpip 0110 netstack
netdrv /sbin/network
ui /sbin/term 0
	/dev/term0 0770 ui
root /bin/login TERM=/dev/term0
ui /sbin/uimng /dev/vga
	/dev/uimng 0110 ui
